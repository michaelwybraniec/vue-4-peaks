{
  "name": "vue-4-peaks-with-vuex",
  "version": "0.1.4",
  "private": true,
  "scripts": {
    "start": "node build/dev-server.js",
    "test": "concurrently \"./node_modules/.bin/jest\" \"jest\"",
    "serve": "vue-cli-service serve",
    "build": "concurrently \"vue-cli-service build\" \"node build/dev-server.js\"",
    "lint": "concurrently \"vue-cli-service lint\" \"eslint --ext .js,.vue src\"",
    "backend": "json-server --watch db.json --routes routes.json --port 8888 --delay 250 --middlewares ./node_modules/json-server-reset",
    "dev": "concurrently \"npm run backend\" \"npm run serve\" \"node build/dev-server.js\""
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "4.1.1",
    "@vue/cli-plugin-e2e-cypress": "5.0.0",
    "@vue/cli-plugin-unit-mocha": "4.1.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "axios": "0.19.2",
    "bootstrap": "4.4.1",
    "bootstrap-vue": "2.3.0",
    "core-js": "3.6.4",
    "date-fns": "2.9.0",
    "flush-promises": "1.0.2",
    "jest-mock-axios": "3.2.0",
    "jest-serializer-vue": "2.0.2",
    "jsdom": "16.0.1",
    "jsdom-global": "3.0.2",
    "vue": "^2.6.11",
    "vue-router": "3.1.5",
    "vue-template-compiler": "2.6.11",
    "vue2-transitions": "0.3.0",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "4.1.1",
    "@vue/cli-service": "4.1.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "vue-template-compiler": "^2.6.11",
    "concurrently": "^4.1.1",
    "json-server": "^0.15.0",
    "json-server-reset": "^1.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [],
  "description": "",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "moduleFileExtensions": [
        "js",
        "json",
        "vue"
      ],
      "moduleNameMapper": {
        "^@/(.*)$": "<rootDir>/src/$1"
      },
      "snapshotSerializers": [
        "<rootDir>/node_modules/jest-serializer-vue"
      ]
    }
  }
}